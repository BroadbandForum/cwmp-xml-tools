<?xml version="1.0" encoding="UTF-8"?>
<dm:document
  xmlns:dm="urn:broadband-forum-org:cwmp:datamodel-1-9"
  xmlns:dmr="urn:broadband-forum-org:cwmp:datamodel-report-1-0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
    urn:broadband-forum-org:cwmp:datamodel-1-9
      https://www.broadband-forum.org/cwmp/cwmp-datamodel-1-9.xsd
    urn:broadband-forum-org:cwmp:datamodel-report-1-0
      https://www.broadband-forum.org/cwmp/cwmp-datamodel-report-1-0.xsd"
  spec="urn:example-com:model"
  file="model.xml">

  <import file="tr-069-biblio.xml"/>

  <glossary>
    <description>
      Glossary.
    </description>
    <item id="A">
      <description>This is A.</description>
    </item>
  </glossary>

  <abbreviations>
    <description>
      Abbreviations.
    </description>
    <item id="X">
      <description>This is X.</description>
    </item>
  </abbreviations>

  <model name="Model:1.0">
    <parameter name="Automobile" access="readOnly">
      <description>
        An {{param}} that I bought yesterday.
      </description>
      <syntax>
        <unsignedInt/>
      </syntax>
    </parameter>

    <parameter name="Bicycle" access="readOnly">
      <description>
        A {{param}}. This is much beter thann an {{param|Automobile}},
        and much worse than a {{param|Diesel}}.
      </description>
      <syntax>
        <unsignedInt/>
      </syntax>
    </parameter>
    
    <parameter name="Canoe" access="readOnly">
      <description>
        A {{param}}. So much better than an {{param|Automobile}}, and
        much worse than a {{param|Hanger.FlyingMachine}}.
      </description>
      <syntax>
        <unsignedInt/>
      </syntax>
    </parameter>

    <profile name="Baseline:1">
      <parameter ref="Automobile" requirement="readOnly"/>
    </profile>
  </model>


  <model name="Model:1.1" base="Model:1.0">
    <parameter base="Automobile" access="readWrite">
      <description action="replace">
        An {{param}} that I bought last week.
        
        I should really replace it with a {{param|Bicycle}}.
      </description>
    </parameter>

    <parameter base="Bicycle" access="readOnly">
      <description action="replace">
        A {{param}}. This is much better than an {{param|Automobile}},
        and much worse than a {{param|Diesel}}.
      </description>
      <syntax>
        <unsignedInt/>
      </syntax>
    </parameter>

    <parameter name="Diesel" access="readOnly" version="1.1">
      <description>
        A {{param}}. Terrible.
      </description>
      <syntax>
        <unsignedInt/>
      </syntax>
    </parameter>

    <parameter name="EtypeJaguar" access="readOnly" version="1.1">
      <description>
        An {{param}}. Fun but not worth it? Better buy a {{param|Canoe}}.
      </description>
      <syntax>
        <unsignedInt/>
      </syntax>
    </parameter>

    <object name="Hanger." access="readOnly" minEntries="1" maxEntries="1">
      <description>
        A {{object}}.
      </description>

      <parameter name="FlyingMachine" access="readOnly">
        <description>
          A {{param}}.
        </description>
        <syntax>
          <unsignedInt/>
        </syntax>
      </parameter>
      
      <command name="Clean()" async="true">
        <description>
          Clean the {{object}}.
        </description>
        <input>
          <parameter name="Depth">
            <description>
              How deep to clean.
            </description>
            <syntax>
              <unsignedInt>
                <units value="inches"/>
              </unsignedInt>
              <default type="parameter" value="42"/>
            </syntax>
          </parameter>
        </input>
      </command>
      
      <event name="Cleaned!">
        <description>
          The {{command|Clean()}} is done.
        </description>
      </event>
    </object>

    <profile name="Advanced:1" extends="Baseline:1">
      <object ref="Hanger." requirement="present">
        <parameter ref="FlyingMachine" requirement="readOnly"/>
      </object>
    </profile>
  </model>
</dm:document>
