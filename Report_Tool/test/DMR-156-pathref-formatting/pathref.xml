<?xml version="1.0" encoding="UTF-8"?>
<dm:document
  xmlns:dm="urn:broadband-forum-org:cwmp:datamodel-1-8"
  xmlns:dmr="urn:broadband-forum-org:cwmp:datamodel-report-1-0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
    urn:broadband-forum-org:cwmp:datamodel-1-8
      https://www.broadband-forum.org/cwmp/cwmp-datamodel-1-8.xsd
    urn:broadband-forum-org:cwmp:datamodel-report-1-0
      https://www.broadband-forum.org/cwmp/cwmp-datamodel-report-1-0.xsd"
  spec="urn:example-com:pathref"
  file="pathref.xml">

  <import file="tr-069-biblio.xml"/>

  <model name="Pathref:1.0">
    <object name="Root." access="readOnly" minEntries="1" maxEntries="1">
      <description>
        Root object.
      </description>
      <parameter name="PoolNumberOfEntries" access="readOnly">
        <description>
          {{numentries}}
        </description>
        <syntax>
          <unsignedInt/>
        </syntax>
      </parameter>
    </object>
    
    <object name="Root.Pool.{i}." access="readOnly" minEntries="0" maxEntries="unbounded"
      numEntriesParameter="PoolNumberOfEntries" dmr:noUniqueKeys="true">
      <description>
        Object pool.
      </description>
    </object>

    <object name="Root.Tool." access="readOnly" minEntries="1" maxEntries="1">
      <description>
        {{object}}.
      </description>
      <parameter name="Reference" access="readOnly">
        <description>
          Reference.
        </description>
        <syntax>
          <string>
            <pathRef refType="strong" targetType="row" targetParent="#.Pool."/>
          </string>
        </syntax>
      </parameter>
      <parameter name="ListReference" access="readOnly">
        <description>
          List reference.
        </description>
        <syntax>
          <list/>
          <string>
            <pathRef refType="strong" targetType="row" targetParent="#.Pool."/>
          </string>
        </syntax>
      </parameter>
      <parameter name="ListReference2" access="readOnly">
        <description>
          {{reference||delete}}
          List reference with 'delete' option.
        </description>
        <syntax>
          <list/>
          <string>
            <pathRef refType="strong" targetType="row" targetParent="#.Pool."/>
          </string>
        </syntax>
      </parameter>
    </object>
  </model>
</dm:document>
